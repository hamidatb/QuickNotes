{
"manifest_version": 3,
"name": "Quick Notes",
"version": "1.0",
"description": "A simple notepad for quick notes linked to specific websites.",
"permissions": ["storage", "tabs"],
"action": {
    "default_popup": "html/popup.html",
    "default_icon": "icons/icon.png"
},
"icons": {
    "48": "icons/icon.png"
},
"default_locale": "en"
}
